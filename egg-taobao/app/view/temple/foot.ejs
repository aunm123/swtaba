		// })
		// cb.on('error', function(e) {
		//    alert('复制失败,请手动复制');
		// });
	}

	function searchAction() {
		var key = $('#search-input').val();
		if (key.length <= 0) {
			return;
		}
		key = key ? key : ""
		window.location.href = "/search?q=" + key;

		var arrayList = JSON.parse(localStorage.getItem("aitt_history")) || []
		if (arrayList.length > 9) {
			arrayList.pop();
			arrayList.unshift(key);
		} else {
			arrayList.unshift(key);
		}
		localStorage.setItem("aitt_history", JSON.stringify(arrayList))
	}

</script>

