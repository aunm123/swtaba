<script>
	$(function () {

		$("img").each(function (index,item) {
			var data_src = $(item).attr('data-src');
			var src = $(item).attr('src');
			if (data_src){
				data_src = data_src.replace('_.webp', "");
				$(item).attr('data-src', data_src);
            }
            if (src){
	            src = src.replace('_.webp', "");
	            $(item).attr('src', src);
            }
		});
		$("img").each(function (index,item) {
			var data_src = $(item).attr('data-ks-lazyload');
			var src = $(item).attr('src');
			if (data_src){
				data_src = data_src.replace('_.webp', "");
				$(item).attr('data-ks-lazyload', data_src);
			}
			if (src){
				src = src.replace('_.webp', "");
				$(item).attr('src', src);
			}
		});

		setTimeout(function () {
			$('img').lazyload({
				data_attribute: 'ks-lazyload',
				effect: 'fadeIn',
				threshold: 180 ,
				container: $("body"),
			});
			$('img').lazyload({
				data_attribute: 'src',
				effect: 'fadeIn',
				threshold: 180,
				container: $("body"),
			});
		},100)
	})

	function open_taobao(targetClass, s_url) {
		// var url = "taobao://s.click.taobao.com/6ERCbL32kLy";                   //淘宝
		var temp = s_url.replace("https:", "");
		temp.repeat("http:", "");
		window.location = "taobao:" + temp;


