<div class="category-div">
    <% topsell.forEach(function(item){ %>
    <div class="top-sell">
        <a class="item" href="javascript:;" onclick="open_url(event,'/top?topid=<%=item.id%>')">
            <i class="iconfont <%=item.icon%>" style="background-color: <%=item.color%>"></i>
            <span><%=item.cName%></span>
        </a>
    </div>
    <% }); %>
</div>

